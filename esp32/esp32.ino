unsigned char getByte() {
  unsigned char c = 0;
  if(digitalRead(D7)==HIGH) c+=128;
  if(digitalRead(D6)==HIGH) c+=64;
  if(digitalRead(D5)==HIGH) c+=32;
  if(digitalRead(D4)==HIGH) c+=16;
  if(digitalRead(D3)==HIGH) c+=8;
  if(digitalRead(D2)==HIGH) c+=4;
  if(digitalRead(D1)==HIGH) c+=2;
  if(digitalRead(D0)==HIGH) c+=1;
  return c;
}


void moveWiFiStatusToOutBuffer() {
  int i = WiFi.status();

  switch(i) {
    case WL_CONNECTED:
        strcpy(outBuffer,"CONNECTED");
        break;
    case WL_NO_SHIELD:
        strcpy(outBuffer,"NO_SHIELD");
        break;
    case WL_IDLE_STATUS:
        strcpy(outBuffer,"IDLE_STATUS");
        break;
    case WL_NO_SSID_AVAIL:
        strcpy(outBuffer,"NO_SSID_AVAIL");
        break;
    case WL_SCAN_COMPLETED:
        strcpy(outBuffer,"SCAN_COMPLETED");
        break;
    case WL_CONNECT_FAILED:
        strcpy(outBuffer,"CONNECT_FAILED");
        break;
    case WL_CONNECTION_LOST:
        strcpy(outBuffer,"CONNECTION_LOST");
        break;
    case WL_DISCONNECTED:
        strcpy(outBuffer,"DISCONNECTED");
        break;
    default:
        strcpy(outBuffer,"WIFI_STATUS_UNKNOWN");
  }
}

void trim_space(char * str)
{
    int index, i;

    /*
     * Trim leading white spaces
     */
    index = 0;
    while(str[index] == ' ' || str[index] == '\t')
    {
        index++;
    }

    /* Shift all trailing characters to its left */
    i = 0;
    while(str[i + index] != '\0')
    {
        str[i] = str[i + index];
        i++;
    }
    str[i] = '\0'; // Terminate string with NULL


    /*
     * Trim trailing white spaces
     */
    i = 0;
    index = -1;
    while(str[i] != '\0')
    {
        if(str[i] != ' ' && str[i] != '\t')
        {
            index = i;
        }

        i++;
    }

    /* Mark the next character to last non white space character as NULL */
    str[index + 1] = '\0';
}

int startsWith(char* s, char* t) {
  if(strlen(s) < strlen(t)) return 0;

  for(int i = 0; i<strlen(t);i++) {
    if(*(s+i) != *(t+i))
        return 0;
  }

  return 1;
}


char* replace_char(char* str, char find, char replace){
    char *current_pos = strchr(str,find);
    while (current_pos){
        *current_pos = replace;
        current_pos = strchr(current_pos,find);
    }
    return str;
}

void invokeCommand() {

  L1_RED();
  getBuffer[0] = 0x0;

  L1_CYAN();
    
  trim_space(inBuffer);

  L1_VIOLET();

  
  if(startsWith(inBuffer, "SETSSID=")) {
    L1_BLUE();    
    strcpy(ssid, inBuffer+8);
    replace_char(ssid,'!','_');
    Serial.printf("SSID: %s\n",ssid);
    strcpy(outBuffer,"OK");
    L1_GREEN();
  }
  else
  if(startsWith(inBuffer, "PASSWORD=")) {
    L1_BLUE();
    strcpy(password, inBuffer+9);
    Serial.printf("PASSWORD: %s\n",password);
    strcpy(outBuffer,"OK");
    L1_YELLOW();
  }
  else
  if(startsWith(inBuffer, "WIFISTATUS")) {
    Serial.printf("WIFISTATUS: ");
    Serial.flush();
    moveWiFiStatusToOutBuffer();
    Serial.printf(outBuffer);
    Serial.printf("\n");
  }
  else
  if(startsWith(inBuffer, "CONNECT")) {
    Serial.printf("CONNECTING...\n", ssid, password);
    strcpy(outBuffer,"COMMAND_ACCEPTED");
    CONNECTING = 1;
  }
  else
  if(startsWith(inBuffer, "GETLOCALIP")) {
    ip = WiFi.localIP();
    strcpy(outBuffer, ip.toString().c_str());
    Serial.printf("GETLOCALIP: %s\n", outBuffer);
  }
  else
  if(startsWith(inBuffer, "GETGATEWAYIP")) {
    gateway = WiFi.gatewayIP();
    strcpy(outBuffer, gateway.toString().c_str());
    Serial.printf("GATEWAYIP: %s\n", outBuffer);
  }
  else
  if(startsWith(inBuffer, "GETSUBNETMASK")) {
    subnet = WiFi.subnetMask();
    strcpy(outBuffer,subnet.toString().c_str());
    Serial.printf("GETSUBNETMASK: %s\n", outBuffer);
  }
  else
  if(startsWith(inBuffer, "HTTPGET=")) {
    strcpy(getBuffer, inBuffer+8);
    strcpy(outBuffer,"OK");
  }
  else
  if(startsWith(inBuffer, "SEEKHEADER=")) {
    if(strlen(headers) > 0) {
      for(int j=0;j<11;j++) inBuffer[j] = ' ';
      trim_space(inBuffer);
      int i = atoi(inBuffer);
      memcpy(outBuffer,(char*)headers[i*256],255);
      outBuffer[255] = 0x0;
    }
    else {
      strcpy(outBuffer,"EMPTY HEADER");
    }
  }
  else
  if(startsWith(inBuffer, "SEEKHTTPDATA=")) {
    if(strlen(outstream) > 0) {
      for(int j=0;j<13;j++) inBuffer[j] = ' ';
      trim_space(inBuffer);
      int i = atoi(inBuffer);
      memcpy(outBuffer,(char*)outstream[i*256],255);
      outBuffer[255] = 0x0;
    }
    else {
      strcpy(outBuffer,"EMPTY STREAM");
    }
  }
  else
  if(startsWith(inBuffer, "CHECKHTTPCOMPLETE")) {
    if(BUFFERSREADY == 1) 
      strcpy(outBuffer,"TRUE");
    else
      strcpy(outBuffer,"FALSE");
  }
  else {
    strcpy(outBuffer,"INVALID_COMMAND");
    Serial.printf("Command Received: %s, %s\n", inBuffer, outBuffer);
  }

  L1_BLACK();
  delay(500);
  inBuffer[0] = 0x0;
}


void invokeCommandTask(void* p) {
  Serial.printf("inside invokeCommandTask()\n");
  Serial.flush();
  L2_RED();
  invokeCommand();
  L2_GREEN();
  resetWaitLatch();
  L2_BLUE();
  vTaskDelete(NULL);
  L2_WHITE();
}


void IRAM_ATTR wifiOutFunc() {
// this function is invoked when the TRS-80 has requested an OUT 240,X (so we are sending data *FROM* the TRS-80 *TO* the ESP-32)
    //static uint8_t ucParameterToPass;
    TaskHandle_t xHandle = NULL;
    
    //Serial.printf("inside wifiOutFunc\n");Serial.flush();
    dataIn();
    char c = getByte();
    
    if(c == 0x0a || c == 0x0d) {
        Serial.printf("creating invokeCommandTask\n");Serial.flush();
        xTaskCreate(invokeCommandTask, "InvComTask", 2048, NULL, 1, &xHandle);
        configASSERT(xHandle);
    }
    else {
        inBuffer[strlen(inBuffer)+1] = 0x0;
        inBuffer[strlen(inBuffer)] = c;
        resetWaitLatch();
    }
}

void sendByte(unsigned char c) {
  if(c >= 128) {digitalWrite(D7,HIGH); c=c-128;} else {digitalWrite(D7,LOW);}
  if(c >= 64)  {digitalWrite(D6,HIGH); c=c-64 ;} else {digitalWrite(D6,LOW);}
  if(c >= 32)  {digitalWrite(D5,HIGH); c=c-32 ;} else {digitalWrite(D5,LOW);}
  if(c >= 16)  {digitalWrite(D4,HIGH); c=c-16 ;} else {digitalWrite(D4,LOW);}
  if(c >=  8)  {digitalWrite(D3,HIGH); c=c- 8 ;} else {digitalWrite(D3,LOW);}
  if(c >=  4)  {digitalWrite(D2,HIGH); c=c- 4 ;} else {digitalWrite(D2,LOW);}
  if(c >=  2)  {digitalWrite(D1,HIGH); c=c- 2 ;} else {digitalWrite(D1,LOW);}
  if(c >=  1)  {digitalWrite(D0,HIGH); c=c- 1 ;} else {digitalWrite(D0,LOW);}
}


/* jigger the flip flop that's tied to the WAIT* line on the TRS-80 to make sure it's in a known state */
void initFlipFlop() {
  digitalWrite(IC6A_PRE,LOW);
  digitalWrite(IC6A_CLR,LOW);

  digitalWrite(IC6A_PRE,HIGH);
  digitalWrite(IC6A_CLR,LOW);

  digitalWrite(IC6A_PRE,LOW);
  digitalWrite(IC6A_CLR,HIGH);

  digitalWrite(IC6A_PRE,HIGH);
  digitalWrite(IC6A_CLR,LOW);

  digitalWrite(IC6A_PRE,HIGH);
  digitalWrite(IC6A_CLR,HIGH);
}


inline void resetWaitLatch() {
  digitalWrite(IC6A_CLR,LOW);
  delayMicroseconds(1);
  digitalWrite(IC6A_CLR,HIGH);
  delayMicroseconds(1);
}

int ix = -1;
void IRAM_ATTR wifiInFunc() {
// this function is invoked when the TRS-80 has requested an INP() (so we are sending data *FROM* the ESP32 *TO* the TRS-80)
    dataOut();

    String s(outBuffer);
    s.trim();
    s.concat("\n");
    
    ix++;
    if(ix >= strlen(s.c_str()))
       ix = 0;
       
    sendByte(s.c_str()[ix]);
    resetWaitLatch();
}

void debugLED(char c) {
  if(c % 128 < 64) digitalWrite(2,LOW);  else digitalWrite(2,HIGH);     // ESP32 built in
  if(c % 64 < 32)  digitalWrite(21,LOW); else digitalWrite(21,HIGH);    // L1 - red
  if(c % 32 < 16)  digitalWrite(0,LOW);  else digitalWrite(0,HIGH);     // L1 - green
  if(c % 16 < 8)   digitalWrite(4,LOW);  else digitalWrite(4,HIGH);     // L1 - blue
  if(c % 8  < 4)   digitalWrite(5,LOW);  else digitalWrite(5,HIGH);     // L2 - red
  if(c % 4  < 2)   digitalWrite(18,LOW); else digitalWrite(18,HIGH);    // L2 - green
  if(c % 2  < 1)   digitalWrite(19,LOW); else digitalWrite(19,HIGH);    // L2 - blue
}


