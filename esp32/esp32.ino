unsigned int iLEDCtr = 0;


void detachTRS80Interrupts() {
  if(interruptsAttached) {
     Serial.printf("Detaching WIFI_OUT interrupt\n");  Serial.flush();
     detachInterrupt(digitalPinToInterrupt(WIFI_OUT));
     Serial.printf("Detaching WIFI_IN interrupt\n");  Serial.flush();
     detachInterrupt(digitalPinToInterrupt(WIFI_IN));
     interruptsAttached = 0;
  }
}

void attachTRS80Interrupts() { 
    if(!interruptsAttached) {
        Serial.printf("Attaching WIFI_OUT interrupt\n");  Serial.flush();
        attachInterrupt(digitalPinToInterrupt(WIFI_OUT), wifiOutFunc, FALLING);
        Serial.printf("Attaching WIFI_IN interrupt\n");  Serial.flush();
        attachInterrupt(digitalPinToInterrupt(WIFI_IN), wifiInFunc, FALLING);
        interruptsAttached = 1;
    }
}


void setup() {
    inBuffer[0] = 0x0;
    strcpy(outBuffer,"UNKNOWN");
    
    L1_BLACK();
    L2_BLACK();
    Serial.begin(115200);
    pinMode(2,OUTPUT);
    
    L2_RED();
    do {   // attempt to establish serial connection
      iLEDCtr++;
      delay(200);
      L2_YELLOW();
      digitalWrite(2,HIGH);
      delay(200);
      L2_RED();
      digitalWrite(2,LOW);
    }
    while(!Serial && iLEDCtr < 30);

    if(!Serial) {} else {
        L2_GREEN();
    }

    Serial.printf("Setting pin direction\n");  Serial.flush();
    setPinDirection();
    initFlipFlop();

    WiFi.mode(WIFI_MODE_STA); 

    attachTRS80Interrupts();

    WiFi.disconnect(true); 
    delay(1000);

    
    WiFi.onEvent(WiFiEvent);


    L1_GREEN();
    getBuffer[0] = 0x0;
    
    Serial.printf("setup() complete.\n");  Serial.flush();
}

void loop() {
  int stat;
  
  //L2_RED(); delay(500); L2_BLACK(); delay(500);
  if(RETRIEVINGSTREAM == 1) {
    Serial.printf("Retrieving data...\n");
    yield();
    if(client.available()) {
      if(strlen(outstream) < 8191) {
        char c = client.read();
        outstream[strlen(outstream)] = c;
        outstream[strlen(outstream)+1] = 0x0;
      }
    }
    else {
      RETRIEVINGSTREAM = 0;
      client.stop();
      BUFFERSREADY = 1; 
      Serial.printf("Data read complete.\n");
    }
  }

  if(RETRIEVINGHEADERS == 1) {
    Serial.printf("Retrieving headers...\n");
    yield();
    if(!client.connected()) {
      RETRIEVINGHEADERS = 0;    
      RETRIEVINGSTREAM = 0;
      Serial.printf("client disconnect during header retrieval\n");
    }
    yield();
    String line = client.readStringUntil('\n');
    if(line == "\r") {
      RETRIEVINGHEADERS = 0;    
      RETRIEVINGSTREAM = 1;
    }
    strcat(headers,line.c_str());
    strcat(headers,"\n");
  }

  if(INVOKINGGET == 1) {
    Serial.printf("Invoking GET...\n");
    INVOKINGGET = 0;
    yield();
    client.printf("GET %s HTTP/1.0\n", getBuffer);
    client.printf("Connection: close\n");
    client.printf("\n");
    BUFFERSREADY = 0;
    RETRIEVINGHEADERS = 1;
    RETRIEVINGSTREAM = 0;
    headers[0] = 0x0;
    outstream[0] = 0x0;
  }

  if(strlen(getBuffer) > 0 && SETTINGCACERT == 0) {
    strcpy(server, getBuffer);
    String s(server);
    s.trim();
    s.replace("https://","");
    s.replace("http://","");
          
    if(s.indexOf("/") > 0) {
      s = s.substring(0,s.indexOf("/"));
    }
    else { 
        if(s.indexOf(" ") > 0) {
          s = s.substring(0,s.indexOf(" "));
        }
    }
    s.trim();
    strcpy(server, s.c_str());
    Serial.printf("Connecting to server %s...\n", server);
    yield();
    if(!client.connect(server, 80)) {
      Serial.printf("Connection failed requesting %s\n", getBuffer);
      getBuffer[0] = 0x0;
      strcpy(outBuffer,"CONNECTION FAILED");
    }
    else {
      INVOKINGGET = 1;
    }
  }

  if(SETTINGCACERT == 1) {
    Serial.printf("Setting Root CA Cert...\n");
    client.setCACert(globalsign_ca);
    SETTINGCACERT = 0;
  }

  if(WAITINGFORCONNECT == 1) {
    yield();
    wl_status_t t = WiFi.status();
    if(t == WL_CONNECTED) {
      WAITINGFORCONNECT = 0;
      SETTINGCACERT = 1;
    }
  }

  if(CONNECTING == 1) {
    CONNECTING = 0;
    WiFi.disconnect(true);
    delay(500);
    Serial.printf("WiFi.begin(%s,%s)...\n", ssid, password);
    //detachTRS80Interrupts();
    WiFi.begin(ssid, password);
    WAITINGFORCONNECT = 1;
  }
}
