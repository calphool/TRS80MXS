void IRAM_ATTR dataIn() {
  if(dataDirection != IN) {
    //Serial.printf("  D7-D0 = INPUT\n");  Serial.flush();
    pinMode(D7, INPUT);
    pinMode(D6, INPUT);
    pinMode(D5, INPUT);
    pinMode(D4, INPUT);
    pinMode(D3, INPUT);
    pinMode(D2, INPUT);
    pinMode(D1, INPUT);
    pinMode(D0, INPUT);
    dataDirection = IN;
  }
}

void IRAM_ATTR dataOut() {
  if(dataDirection != OUT) {
    //Serial.printf("  D7-D0 = OUTPUT\n");  Serial.flush();
    pinMode(D7, OUTPUT);
    pinMode(D6, OUTPUT);
    pinMode(D5, OUTPUT);
    pinMode(D4, OUTPUT);
    pinMode(D3, OUTPUT);
    pinMode(D2, OUTPUT);
    pinMode(D1, OUTPUT);
    pinMode(D0, OUTPUT);
    dataDirection = OUT;
  }
}

